[alias]
  aa = add --all
  ai = add --all --intent-to-add
  ap = add --patch
  b = branch
  ca = commit --amend
  ci = commit -v
  co = checkout
  fuckit = !git add --all && git reset --hard
  graph = log --oneline --graph--all --decorate
  wip = !git add --all && git commit -m wip
  s = status
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[fetch]
  prune = true
[merge]
  ff = only
[push]
  default = current
[rebase]
  autosquash = true
